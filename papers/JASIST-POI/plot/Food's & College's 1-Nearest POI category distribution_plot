
set boxwidth 1 relative
set style data histogram
set style histogram cluster
set style fill pattern 3 border lt -1
set key title ""
set key inside right top
set size ratio 0.618
set xlabel "Category" offset 0,1.5
set ylabel "Probability" offset 1.5,0
set xtics rotate by 45 offset 0,0 right
set term post eps enhanced "Helvetica" 25
	
set output "Food's_&_College's_1-Nearest_POI_category_distribution.eps"
plot "Food's & College's 1-Nearest POI category distribution.txt" using 3:xticlabel(1) title "College & University", "Food's & College's 1-Nearest POI category distribution.txt" using 2:xticlabel(1) title "Food"
