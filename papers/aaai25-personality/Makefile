# # Rule to generate PDF using pdflatex
all: main appendix
main:
	pdflatex main
	bibtex main   # Only if you have bibliography
	pdflatex main
	pdflatex main

appendix:
	pdflatex Appendix
	bibtex Appendix
	pdflatex Appendix
	pdflatex Appendix

# Rule to clean auxiliary files generated by LaTeX
clean: 
	rm -f *.out *.bbl *.synctex.gz *.blg *.aux *.dvi *.log *.ent main.pdf Appendix.pdf *.ps *converted-to.pdf

