plot "Nightlife_Spot_data.txt" using 2:xticlabel(1) title ""
set output "Outdoors_&_Recreation_data.eps"
plot "Outdoors_&_Recreation_data.txt" using 2:xticlabel(1) title ""
set output "Professional_&_Other_Places_data.eps"
plot "Professional_&_Other_Places_data.txt" using 2:xticlabel(1) title ""
set output "Residence_data.eps"
plot "Residence_data.txt" using 2:xticlabel(1) title ""
set output "Shop_&_Service_data.eps"
plot "Shop_&_Service_data.txt" using 2:xticlabel(1) title ""
set output "Travel_&_Transport_data.eps"
plot "Travel_&_Transport_data.txt" using 2:xticlabel(1) title ""
