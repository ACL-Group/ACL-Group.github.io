set boxwidth 1 relative
set style data histogram
set style histogram cluster
set style fill pattern 3 border lt -1
set key title ""
# set key at 9,0.7 font "Helvetica,20" left Left reverse
set key inside top right
set size ratio 0.618
set xlabel "" offset 0,1
set ylabel "" offset 1.5,0
set xtics rotate by 45 offset 0,0 right
set term post eps enhanced "Helvetica" 25

 set yrange[0:]
set output "FirstClassOneError.eps"
plot "FirstClassOneError_data.txt" using 2:xticlabel(1) title "NewYork", "FirstClassOneError_data.txt" using 3:xticlabel(1) title "Singapore"

set output "FirstClassCoverage.eps"
plot "FirstClassCoverage_data.txt" using 2:xticlabel(1) title "NewYork", "FirstClassCoverage_data.txt" using 3:xticlabel(1) title "Singapore"
