set boxwidth 1 relative
set style data histogram
set style histogram cluster
set style fill pattern 3 border lt -1
set key title ""
#set key at 9,0.7 font "Helvetica,20" left Left reverse
set key inside right top
set size ratio 0.618
set xlabel "Category" offset 0,1
set ylabel "F1 score" offset 1.5,0
set yrange[0:1]
set xtics rotate by 45 offset 0,0 right
set term post eps enhanced "Helvetica" 25

# set yrange[0:1.19]
set output "SingaporeF1.eps"
plot "SingaporeF1_data.txt" using 2:xticlabel(1) title "NAME", "SingaporeF1_data.txt" using 3:xticlabel(1) title "NAME+EP", "SingaporeF1_data.txt" using 4:xticlabel(1) title "NAME+EP+BEST"

set output "NewYorkF1.eps"
plot "NewYorkF1_data.txt" using 2:xticlabel(1) title "NAME", "NewYorkF1_data.txt" using 3:xticlabel(1) title "NAME+EP", "NewYorkF1_data.txt" using 4:xticlabel(1) title "NAME+EP+BEST"
