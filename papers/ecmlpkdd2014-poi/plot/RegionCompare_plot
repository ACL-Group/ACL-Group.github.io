
set boxwidth 1 relative
set style data histogram
set style histogram cluster
set style fill pattern 1 border lt -1
set key title ""
#set key inside right top
set key at 5,3 left Left reverse
set size ratio 0.618
set xlabel "Category" offset 0,3.5
set ylabel "Region Compare (Total check-in)" offset 1.5,0
set xtics offset 0,4 center
set xzeroaxis lw 1 lt 1

set term post eps enhanced "Helvetica" 20
	
set output "RegionCompare.eps"
plot "RegionCompare.txt" using 2:xticlabel(1) title "Arts \\& Entertainment", "RegionCompare.txt" using 3:xticlabel(1) title "Food", "RegionCompare.txt" using 4:xticlabel(1) title "Nightlife Spot", "RegionCompare.txt" using 5:xticlabel(1) title "Outdoors & Recreation", "RegionCompare.txt" using 6:xticlabel(1) title "Shop & Service"
