
set boxwidth 1 relative
set style data histogram
set style histogram cluster
set style fill pattern 3 border lt -1
set key title "Feature"
#set key inside right top font ",15"
set key at 3,0.8 font "Helvetica,15" left Left reverse
set yrange[0:1]
set size ratio 0.618
set xlabel "" offset 0,-1.5
set ylabel "" offset 1.5,0
set xtics rotate by 45 offset 0,0 right
set term post eps enhanced "Helvetica" 25
	
set output "Use_N_k_feature_on_Food.eps"
plot "Use N_k feature on Food.txt" using 2:xticlabel(1) title "BASE", "Use N_k feature on Food.txt" using 3:xticlabel(1) title "BASE+N\\_1", "Use N_k feature on Food.txt" using 4:xticlabel(1) title "BASE+N\\_5", "Use N_k feature on Food.txt" using 4:xticlabel(1) title "BASE+N\\_10", "Use N_k feature on Food.txt" using 4:xticlabel(1) title "BASE+N\\_20"
