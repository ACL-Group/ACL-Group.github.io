@Article{ani8080131,
AUTHOR = {Siniscalchi, Marcello and D’Ingeo, Serenella and Minunno, Michele and Quaranta, Angelo},
TITLE = {Communication in Dogs},
JOURNAL = {Animals},
VOLUME = {8},
YEAR = {2018},
NUMBER = {8},
ARTICLE-NUMBER = {131},
URL = {https://www.mdpi.com/2076-2615/8/8/131},
PubMedID = {30065156},
ISSN = {2076-2615},
ABSTRACT = {Dogs have a vast and flexible repertoire of visual, acoustic, and olfactory signals that allow an expressive and fine tuned conspecific and dog&ndash;human communication. Dogs use this behavioural repertoire when communicating with humans, employing the same signals used during conspecific interactions, some of which can acquire and carry a different meaning when directed toward humans. The aim of this review is to provide an overview of the latest progress made in the study of dog communication, describing the different nature of the signals used in conspecific (dog&ndash;dog) and heterospecific (dog&ndash;human) interactions and their communicative meaning. Finally, behavioural asymmetries that reflect lateralized neural patterns involved in both dog&ndash;dog and dog&ndash;human social communication are discussed.},
DOI = {10.3390/ani8080131}
}

