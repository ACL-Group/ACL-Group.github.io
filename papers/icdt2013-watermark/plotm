#!/usr/bin/gnuplot
#
#    
#    	G N U P L O T
#    	Version 4.6 patchlevel 0    last modified 2012-03-04 
#    	Build System: MS-Windows 32 bit 
#    
#    	Copyright (C) 1986-1993, 1998, 2004, 2007-2012
#    	Thomas Williams, Colin Kelley and many others
#    
#    	gnuplot home:     http://www.gnuplot.info
#    	faq, bugs, etc:   type "help FAQ"
#    	immediate help:   type "help"  (plot window: hit 'h')
set term post eps enhanced "Helvetica" 25
set xtics auto
set ytics auto
set xrange [3:35]
set yrange [0:1]
set output "mparameter.eps"
set key inside bottom left title "Crop Ratio" font "Time-Roman,35"
set xlabel "{/Symbol q} (10km)" font "Time-Roman,45"   
set ylabel "Detection Accuracy" font "Time-Roman,45" 
plot "mparameter.txt" using 1:2 title '0.5' with linespoints lt 1 pt 2 lw 4 ps 2,"mparameter.txt" using 1:3 title '0.25' with linespoints lt 1 pt 4 lw 4 ps 2, "mparameter.txt" using 1:4 title '0.125' with linespoints lt 1 pt 7 lw 4 ps 2


