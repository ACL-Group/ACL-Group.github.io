#!/usr/bin/gnuplot
#
#    
#    	G N U P L O T
#    	Version 4.6 patchlevel 0    last modified 2012-03-04 
#    	Build System: MS-Windows 32 bit 
#    
#    	Copyright (C) 1986-1993, 1998, 2004, 2007-2012
#    	Thomas Williams, Colin Kelley and many others
#    
#    	gnuplot home:     http://www.gnuplot.info
#    	faq, bugs, etc:   type "help FAQ"
#    	immediate help:   type "help"  (plot window: hit 'h')
set term post eps enhanced "Time-Roman" 35
set xtics auto
set ytics auto
set xrange [6.6:212.9]
set yrange [0:1.1]
set output "delta.eps"
set key inside bottom right title "Method"
set xlabel "{{/Symbol d}_w} (10^{-6})"
set ylabel "Result Accuracy"
plot "delta.txt" using 4:5 title 'Jiang' with linespoints lt 1 pt 2 lw 4 ps 2,"delta.txt" using 1:2 title 'Voigt' with linespoints lt 1 pt 4 lw 4 ps 2, "delta.txt" using 7:8 title 'Pu' with linespoints lt 1 pt 5 lw 4 ps 2

set term post eps enhanced "Time-Roman" 35
set xtics auto
set ytics auto
set xrange [6.6:212.9]
set yrange [0:1.1]
set output "deltamerge.eps"
set key at 212,0.7 title "Method"
set xlabel "{{/Symbol d}_w} (10^{-6})"
set ylabel "Result Accuracy"
plot "delta.txt" using 4:6 title 'Jiang' with linespoints lt 1 pt 2 lw 4 ps 2,"delta.txt" using 10:11 title 'Voigt' with linespoints lt 1 pt 4 lw 4 ps 2, "delta.txt" using 7:9 title 'Pu' with linespoints lt 1 pt 5 lw 4 ps 2
