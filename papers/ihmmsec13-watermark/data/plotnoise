#!/usr/bin/gnuplot
#
#    
#    	G N U P L O T
#    	Version 4.6 patchlevel 0    last modified 2012-03-04 
#    	Build System: MS-Windows 32 bit 
#    
#    	Copyright (C) 1986-1993, 1998, 2004, 2007-2012
#    	Thomas Williams, Colin Kelley and many others
#    
#    	gnuplot home:     http://www.gnuplot.info
#    	faq, bugs, etc:   type "help FAQ"
#    	immediate help:   type "help"  (plot window: hit 'h')
set term post eps enhanced "Time-Roman" 35
set xtics auto
set ytics auto
set xrange [15:75]
set yrange [0:1.1]
set key inside bottom left title "Method"
set output "noiseattack.eps"
set xlabel "{/Symbol d}_n * Total Road Length(10km)"
set ylabel "Result Accuracy"
plot "noisedata.txt" using 1:2 title "Jiang" with linespoints lt 1 pt 2 lw 4 ps 2,"noisedata.txt" using 1:3 title "Voigt" with linespoints lt 1 pt 4 lw 4 ps 2,"noisedata.txt" using 1:4 title "Pu" with linespoints lt 1 pt 5 lw 4 ps 2


