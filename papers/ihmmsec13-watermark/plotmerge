#!/usr/bin/gnuplot
#
#    
#    	G N U P L O T
#    	Version 4.6 patchlevel 0    last modified 2012-03-04 
#    	Build System: MS-Windows 32 bit 
#    
#    	Copyright (C) 1986-1993, 1998, 2004, 2007-2012
#    	Thomas Williams, Colin Kelley and many others
#    
#    	gnuplot home:     http://www.gnuplot.info
#    	faq, bugs, etc:   type "help FAQ"
#    	immediate help:   type "help"  (plot window: hit 'h')
set term post eps enhanced "Helvetica" 25
set xtics (0.75,0.5,0.25,0.125,0.0625)
set ytics auto
set log x 2
set key inside bottom right title "Method" font "Time-Roman,35"
set xrange [0.0625:0.75]
set yrange [0:1.05]
set output "mergeattack.eps"
set xlabel "Watermarked Map Ratio(log scale)" font "Time-Roman,35"
set ylabel "Detect Accuracy" font "Time-Roman,35"
plot "merge.txt" using 1:2 title "Jiang" with linespoints lt 1 pt 2 lw 4 ps 2,"merge.txt" using 1:3 title "Voigt" with linespoints lt 1 pt 4 lw 4 ps 2,"merge.txt" using 1:4 title "Pu" with linespoints lt 1 pt 5 lw 4 ps 2


