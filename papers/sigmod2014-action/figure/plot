#    	faq, bugs, etc:   type "help FAQ"
#    	immediate help:   type "help"  (plot window: hit 'h')
set term post eps enhanced "Helvetica" 25
unset logscale x
set xtics auto
set ytics auto
set xrange [0:11]
set output "C:\\Paper\\sigmod2014-action\\figure\\argument_identify.eps"
set key inside top right title "Method"
set xlabel "Number of Concepts(k)" offset 0,0.5
set ylabel "Precision" offset 1.5,0
plot "C:\\Paper\\sigmod2014-action\\figure\\argument_identify_data.txt" using 1:2 title 'Action' with linespoints lt 1 pt 2 lw 6 ps 2, "C:\\Paper\\sigmod2014-action\\figure\\argument_identify_data.txt" using 1:3 title 'ReVerb' with linespoints lt 3 pt 2 lw 6 ps 0, "C:\\Paper\\sigmod2014-action\\figure\\argument_identify_data.txt" using 1:4 title 'SRL(Semafor)' with linespoints lt 4 pt 4 lw 6 ps 0, "C:\\Paper\\sigmod2014-action\\figure\\argument_identify_data.txt" using 1:5 title 'SP' with linespoints lt 4 pt 4 lw 6 ps 2
