#!/gnuplot
#
#    
#    	G N U P L O T
#    	Version 4.6 patchlevel 0    last modified 2012-03-04 
#    	Build System: MS-Windows 32 bit 
#    
#    	Copyright (C) 1986-1993, 1998, 2004, 2007-2012
#    	Thomas Williams, Colin Kelley and many others
#    
#    	gnuplot home:     http://www.gnuplot.info
#    	faq, bugs, etc:   type "help FAQ"
#    	immediate help:   type "help"  (plot window: hit 'h')
set term post eps enhanced "Helvetica" 25
set logscale x
set xtics auto
set ytics auto
set xrange [1:90]
set output "iterateTime.eps"
set key inside top right title "Sample Size"
set xlabel "Iterations (Log Scale)" offset 0,0.5
set ylabel "Time (thousands of seconds)" offset 1.5,0
plot "iterateTime.txt" using 1:($2/1000) title '5000' with linespoints lt 1 pt 2 lw 4 ps 2, "iterateTime.txt" using 1:($3/1000) title '10000' with linespoints lt 1 pt 3 lw 4 ps 2, "iterateTime.txt" using 1:($4/1000) title '15000' with linespoints lt 1 pt 4 lw 4 ps 2, "iterateTime.txt" using 1:($5/1000) title '20000' with linespoints lt 1 pt 5 lw 4 ps 2, "iterateTime.txt" using 1:($6/1000) title '35000' with linespoints lt 1 pt 6 lw 4 ps 2, "iterateTime.txt" using 1:($7/1000) title '50000' with linespoints lt 1 pt 7 lw 4 ps 2
